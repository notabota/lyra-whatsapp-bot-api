generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model chats {
  id   String  @id
  name String?
}

model contact_chat {
  id         Int     @id @default(autoincrement())
  contact_id String?
  chat_id    String?
}

model contacts {
  id        String  @id
  name      String?
  number    String?
  pushname  String?
  shortName String?
}

model messages {
  id        String  @id
  body      String?
  author    String?
  from      String?
  to        String?
  type      String?
  data      String?
  filename  String?
  mimetype  String?
  filesize  Int?
  timestamp Int?
}
